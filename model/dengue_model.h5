import numpy as np
import tensorflow as tf
from tensorflow.keras.models import Sequential
from tensorflow.keras.layers import Dense
from tensorflow.keras.models import save_model

# Membuat model dummy
model = Sequential([
    Dense(64, input_dim=7, activation='relu'),
    Dense(32, activation='relu'),
    Dense(1, activation='sigmoid')  # Output untuk klasifikasi biner (positif/negatif)
])

# Menginisialisasi bobot secara acak
model.set_weights([np.random.rand(*w.shape) for w in model.get_weights()])

# Menyimpan model dummy ke dalam file .h5
model.save("dengue_model.h5")
print("Model dummy telah disimpan sebagai 'dengue_model.h5'")
